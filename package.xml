<?xml version="1.0"?>
<package packagerversion="1.4.11" version="2.0" xmlns="http://pear.php.net/dtd/package-2.0" xmlns:tasks="http://pear.php.net/dtd/tasks-1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://pear.php.net/dtd/tasks-1.0
http://pear.php.net/dtd/tasks-1.0.xsd
http://pear.php.net/dtd/package-2.0
http://pear.php.net/dtd/package-2.0.xsd">
    <name>memcached</name>
    <channel>pecl.php.net</channel>
    <summary>PHP extension for interfacing with memcached via libmemcached library</summary>
    <description>
        A PHP extension for metrics collect
    </description>
    <lead>
        <name>Maksim Novikov</name>
        <user>xMlex</user>
        <email>the.mlex@gmail.com</email>
        <active>yes</active>
    </lead>
    <date>2024-06-18</date>
    <version>
        <release>0.0.6-dev</release>
        <api>3.3.0</api>
    </version>
    <stability>
        <release>beta</release>
        <api>stable</api>
    </stability>
    <license uri="http://www.php.net/license">PHP</license>
    <notes>
        - support PHP 8+
    </notes>
    <contents>
        <dir name="/">
            <file role='doc' name='README.md'/>
            <file role='doc' name='LICENSE'/>
            <file role='src' name='config.m4'/>
            <file role='src' name='config.w32'/>
            <file role='src' name='trochilidae.c'/>
            <file role='src' name='php_trochilidae.h'/>
            <file role='src' name='trochilidae_arginfo.h'/>
            <dir name="trochilidae">
                <file role='src' name='tr_network.c'/>
                <file role='src' name='tr_network.h'/>
                <file role='src' name='tr_timer.c'/>
                <file role='src' name='tr_timer.h'/>
                <file role='src' name='utils.c'/>
                <file role='src' name='utils.h'/>
            </dir>
        </dir>
    </contents>
    <dependencies>
        <required>
            <php>
                <min>7.4.0</min>
            </php>
            <pearinstaller>
                <min>1.4.0b1</min>
            </pearinstaller>
        </required>
    </dependencies>
    <providesextension>trochilidae</providesextension>
    <extsrcrelease>
        <configureoption name="with-zlib-dir"             prompt="zlib directory"             default="no"/>
    </extsrcrelease>
    <changelog>
        <release>
            <date>2024-06-18</date>
            <version>
                <release>0.0.6-dev</release>
                <api>3.2.0</api>
            </version>
            <stability>
                <release>stable</release>
                <api>stable</api>
            </stability>
            <license uri="http://www.php.net/license">PHP</license>
            <notes>
                - PHP 8.0 and 8.2 support
            </notes>
        </release>
    </changelog>
</package>